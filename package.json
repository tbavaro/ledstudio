{
  "name": "ledstudio",
  "homepage": ".",
  "version": "0.25.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "google-palette": "^1.1.0",
    "midievents": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "three": "^0.132.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "npm run codegen && react-scripts start",
    "build": "npm run codegen && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "publish": "./node_modules/.bin/tsdevtools deploy --branch gh-pages --subtreeDir build",
    "fcserver": "./fcserver/fcserver.sh",
    "codegen": "tsc src/codegen/GenerateVisualizationRegistry.ts && node src/codegen/GenerateVisualizationRegistry.js",
    "prettier": "prettier --write src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/three": "^0.131.0",
    "@types/webmidi": "^2.0.6",
    "handlebars": "^4.7.7",
    "import-sort-style-module": "^6.0.0",
    "node": "^16.6.1",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "tsdevtools": "github:tbavaro/tsdevtools#dist",
    "typescript": "^4.4.2"
  },
  "prettier": {
    "trailingComma": "none",
    "arrowParens": "avoid",
    "singleQuote": false,
    "overrides": [
      {
        "files": "*.hbs",
        "options": {
          "htmlWhitespaceSensitivity": "strict"
        }
      }
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
